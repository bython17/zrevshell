  "version": "0.2.0",
  "configurations": [
    {
      "name": "zrevshell server",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": [
        "--connect-ip",
        "127.0.0.1",
        "-pf",
        "server_data/profile.json",
        "-sd",
        "server_data/data.db",
        "-d"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
